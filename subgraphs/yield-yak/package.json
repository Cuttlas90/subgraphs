{
  "name": "reinvest-tracker",
  "license": "MIT",
  "version": "0.1.0",
  "scripts": {
    "build-and-deploy:full": "yarn mustache:full && yarn codegen && yarn build && yarn deploy:full",
    "build-and-deploy:liboutech": "yarn mustache:light && yarn codegen && yarn build && yarn deploy-liboutech:light",
    "build-contract": "solc contracts/Gravity.sol --abi -o abis --overwrite && solc contracts/Gravity.sol --bin -o bin --overwrite",
    "codegen": "graph codegen",
    "build": "graph build",
    "deploy:full": "graph deploy --product hosted-service Cuttlas90/yieldyak",
    "deploy-liboutech:light": "graph deploy liboutech/reinvest-light --ipfs https://api.thegraph.com/ipfs/ --node https://api.thegraph.com/deploy/",
    "mustache:full": "yarn mustache subgraph_data.json subgraph.yaml.mustache > subgraph.yaml",
    "mustache:light": "node mustache.js",
    "upload": "yarn codegen && yarn deploy:full"
  },
  "devDependencies": {
    "@graphprotocol/graph-cli": "^0.29.0",
    "@graphprotocol/graph-ts": "^0.26.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "babel-register": "^6.26.0",
    "js-yaml": "^4.1.0",
    "mustache": "^4.2.0",
    "truffle": "^5.0.4",
    "truffle-contract": "^4.0.5",
    "truffle-hdwallet-provider": "^1.0.4",
    "web3": "^1.5.2"
  },
  "main": "index.js",
  "repository": "https://github.com/yieldyak/subgraph-avalanche.git",
  "author": "Cuttlas <mohsenbarekati@gmail.com>"
}
